{
  "root": true,
  "parser": "babel-eslint",
  "plugins": ["react", "prettier"],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "google",
    "prettier",
    "prettier/react"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "jasmine": true,
    "node": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "rules": {
    "prettier/prettier": ["error", {"singleQuote": true}],
    "require-jsdoc": "off",
    "comma-dangle": "off",
    "arrow-parens": "off",
    "no-console": "off"
  }
}
