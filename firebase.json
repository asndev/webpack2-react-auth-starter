{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "target",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(css|html|js|map)",
        "headers": [
          {"key": "Cache-Control", "value": "max-age=3600"}
        ]
      }
    ]
  }
}
